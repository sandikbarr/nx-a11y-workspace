{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@a11y/ui-components": ["libs/shared/ui-components/index.ts"],
      "@a11y/ui-components/expansion-menu": [
        "libs/shared/ui-components/expansion-menu/index.ts"
      ],
      "@a11y/ui-components/form/checkbox": [
        "libs/shared/ui-components/form/checkbox/index.ts"
      ],
      "@a11y/ui-components/form/input": [
        "libs/shared/ui-components/form/input/index.ts"
      ],
      "@a11y/ui-components/popup-menu": [
        "libs/shared/ui-components/popup-menu/index.ts"
      ],
      "@a11y/ui-components/nav": [
        "libs/shared/ui-components/nav/index.ts"
      ],
      "@a11y/ui-components/nav/horizontal-nav": [
        "libs/shared/ui-components/nav/horizontal-nav.ts"
      ],
      "@a11y/ui-components/nav/vertical-nav": [
        "libs/shared/ui-components/nav/vertical-nav.ts"
      ],
      "@nx-a11y-workspace/nx-a11y-demo/angular-eslint": [
        "libs/nx-a11y-demo/angular-eslint/src/index.ts"
      ],
      "@nx-a11y-workspace/nx-a11y-demo/testing-library": [
        "libs/nx-a11y-demo/testing-library/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
